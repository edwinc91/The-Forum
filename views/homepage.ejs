<h1> Welcome </h1>
<% threads.forEach(function (thread, i) { %>
  <p class = "thread">
    <h3>
      <a href="/threads/<%= thread._id %>"><%= thread.title %> </a>
    </h3>
    <h6>
      <a href="/threads/<%= thread._id %>"><%= thread.comments.length %> comments </a>
    </h6>
  </p>
<% }) %>
<h6>
  <a href="/login">Log in</a>
</h6>
